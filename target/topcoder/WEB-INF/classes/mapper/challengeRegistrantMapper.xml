<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.buaa.act.sdp.dao.ChallengeRegistrantDao">
    <insert id="insert" parameterType="com.buaa.act.sdp.bean.challenge.ChallengeRegistrant" useGeneratedKeys="true" keyProperty="id">
        insert into challenge_registrant(challengeID,handle,reliability,registrationDate,submissionDate,rating) values
        <foreach collection="array" item="item" index="index" separator=",">
        (#{challengeID},#{handle},#{reliability},#{registrationDate},#{submissionDate},#{rating})
        </foreach>
    </insert>
</mapper>